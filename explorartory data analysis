import pandas as pd
import numpy as np
import seaborn as sns
import matplotlib.pyplot as plt

df = pd.read_csv("C://Users//Rahul//OneDrive//Desktop//data science 101//python file//germany-cars-zenrows.csv")
### for scatter plot ###########
print(df["price"])
x =df['hp']
y= (df['price'])

plt.title("scatterplot of hp vs price")
plt.xlabel("hp")
plt.ylabel("price")
plt.scatter(x,y)
plt.show()




